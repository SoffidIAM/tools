<?xml version="1.0" encoding="UTF-8"?>
<refactorings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="jascut JaSCUTConfig.xsd"
    xmlns="jascut">

    <change-method-signature>
        <type>java.lang.Object</type>
        <method-orig>clone</method-orig>
        <args-orig></args-orig>
        <mods-new>static</mods-new>
        <return-new>java.lang.Exception</return-new>
        <method-new>CLONE</method-new>
        <args-new></args-new>
        <throws-new>java.lang.Exception</throws-new>
    </change-method-signature>

    <rename-method>
        <description>printf to printf2</description>
        <type>java.io.PrintStream</type>
        <method-orig>printf</method-orig>
        <args-orig>java.lang.String, java.lang.Object...</args-orig>
        <method-new>printf2</method-new>
    </rename-method>
